;
; Problem1: move
;
; Author: Mark Schmieg
;
(define (problem problem1)
  (:domain subExploration)
  
  (:objects Captain Navigator Engineer Scientist ComOfficer
    SeaPort loc1 loc2 loc3 loc4
    ManAuv InspecAuv
    SubDB SeaportDB
    Target1 Artifact1
    GiantSquid
    Report Pic
  )
  
  
  (:init
    (SubAt SeaPort)
    (SubNotBeen loc1)
    (SubNotBeen loc2)
    (SubNotBeen loc3)
    (SubResurface)
    
    (ArtLoc Artifact1 loc2)
    (HandEmpty)
    (AuvInBay ManAuv)
    (AuvInBay InspecAuv)
    (GiantSquidLoc loc1)
  
  )
  
  
  (:goal
    (and
        (SubRoute SeaPort loc3)
        (SubRoute loc3 loc2)
        (SubRoute loc2 loc1)
        (TransferedToDB Report Artifact1 SeaportDB)
        ;(TransferedToDB Report Artifact2 SeaportDB)
        ;(TransferedToDB Pic Target1 SeaportDB)
       
        
    )
  
  
  )

)